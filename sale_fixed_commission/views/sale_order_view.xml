<?xml version="1.0" encoding="utf-8"?>
<odoo>
 
    <record id="view_order_agent_form_inherited" model="ir.ui.view">
        <field name="name">sale.agent.order.inherited.form</field>
        <field name="model">sale.order</field>
        <field name="inherit_id" ref="sale.view_order_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='partner_id']" position="after">
                <field name="commission" invisible="1"  />
            </xpath>
           <xpath expr="/form/sheet/notebook/page/field[@name='order_line']" position="attributes">
                <attribute name="context">{'default_commission':commission}</attribute>
          </xpath>
          <xpath expr="/form/sheet/notebook/page/field[@name='order_line']//form//field[@name='price_unit']" position="after">
                <field name="commission" invisible="1"  />
          </xpath>  
        </field>
    </record>
  
 
 
</odoo>
